{
  "name": "zillion.ai",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://zillion-ai.github.io/homepage",
  "main": "electron.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:web": "react-scripts build",
    "build:electron": "cross-env PUBLIC_URL=. react-scripts build",
    "electron:start": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=true electron .\"",
    "electron:serve": "npm run build:electron && npx electron ./dist-electron/electron.js",
    "electron:dist": "npm run build:electron && electron-builder"
  },
  "build": {
    "appId": "ai.zillion",
    "files": [
      "build/**/*",
      "electron.ts",
      "preload.ts"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "lucide-react": "^0.541.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "gh-pages": "^6.3.0",
    "wait-on": "^8.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
